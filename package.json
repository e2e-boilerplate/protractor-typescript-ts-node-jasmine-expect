{
  "name": "protractor-typescript-ts-node-jasmine-expect",
  "version": "1.0.0",
  "description": "Protractor end-to-end test automation boilerplate, using TypeScript, ts-node, Jasmine and Expect",
  "scripts": {
    "lint": "npx tslint -c tslint.json './spec/*.ts'",
    "pretty": "npx prettier --no-config './spec/*.ts' --write",
    "upadte-webderiver": "npx webdriver-manager update",
    "upadte-webderiver-ci": "npx webdriver-manager update --versions.chrome=79.0.3945.36",
    "test": "npx protractor"
  },
  "keywords": [
    "protractor",
    "es6",
    "typescript",
    "ts-node",
    "boilerplate",
    "ts-node",
    "example",
    "e2e",
    "e2e test"
  ],
  "author": "Girma Nigusse",
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "3.5.7",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "12.12.29",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "protractor": "5.4.3",
    "ts-node": "8.6.2",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.8.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
