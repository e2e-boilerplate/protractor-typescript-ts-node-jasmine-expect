{
  "name": "protractor-es6modules-typescript-tsnode-jasmine",
  "version": "1.0.0",
  "description": "Protractor end-to-end test automation boilerplate project. Using ES6 modules, TypeScript,ts-node and jasmine",
  "scripts": {
    "upadte-webderiver": "npx webdriver-manager update",
    "test": "npx protractor ./protractor.conf.js",
    "lint": "npx tslint -c tslint.json './spec/*.ts'",
    "pretty": "npx prettier --no-config './spec/*.ts' --write"
  },
  "keywords": [
    "protractor",
    "es6 module",
    "typescript",
    "ts-node",
    "boilerplate",
    "ts-node",
    "example"
  ],
  "author": "Girma Nigusse",
  "license": "MIT",
  "devDependencies": {
    "@types/jasmine": "3.4.5",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "12.12.6",
    "husky": "3.0.9",
    "lint-staged": "9.4.2",
    "prettier": "1.18.2",
    "pretty-quick": "2.0.0",
    "protractor": "5.4.2",
    "ts-node": "8.4.1",
    "tslint": "5.20.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "3.6.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
